@model List<BaoMoi.Models.category3>

@foreach (var i in Model)
{
    <div>
        <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4"><a href="@ViewBag.meta/@i.meta/">@i.name</a></div>
    </div>
    <div class="clearfix"></div>
    <div class="fh5co_tags_all">

        @Html.Action("getTags", "Blogs", new { id = i.id });

    </div>
}
