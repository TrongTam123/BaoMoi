@{
    ViewBag.Titile = "Trending";
    Layout = "~/Views/Shared/_MyLayout.cshtml";

}


<div class="container paddding">
    <div class="row mx-0">
        
        <div class="col-md-3 animate-box" data-animate-effect="fadeInRight">
            <div>
                <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">@Model.name</div>
            </div>
            <div class="clearfix"></div>
            <div class="fh5co_tags_all">
                @Html.Action("getTags", "Default", new { id = Model.id });
            </div>

        </div>
    </div>
    
</div>