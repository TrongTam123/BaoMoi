@model  IEnumerable<BaoMoi.Models.NewsTiep>

<div class="row">
    @foreach (var i in Model)
    {
    <div class="col-md-6 col-6 paddding animate-box" data-animate-effect="fadeIn">
        <div class="fh5co_suceefh5co_height_2">
            <img src="~/Content/images/@i.img" alt="img" />
            <div class="fh5co_suceefh5co_height_position_absolute"></div>
            <div class="fh5co_suceefh5co_height_position_absolute_font_2">
                <div class="">
                    <a href="#" class="color_fff">
                        <i class="fa fa-clock-o"></i>&nbsp;&nbsp;@i.description
                    </a>
                </div>
                <div class=""><a href="/@ViewBag.meta/@i.meta/@i.id" class="fh5co_good_font_2"> @i.name <br>@i.detail </a></div>
            </div>
        </div>
    </div>
    }

</div>

