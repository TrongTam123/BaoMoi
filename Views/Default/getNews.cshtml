@model IEnumerable<BaoMoi.Models.News>


@foreach (var i in Model)
{

    <img src="~/Content/images/@i.img" alt="img" />
    <div class="fh5co_suceefh5co_height_position_absolute"></div>
    <div class="fh5co_suceefh5co_height_position_absolute_font">
        <div class="">
            <a href="#" class="color_fff">
                <i class="fa fa-clock-o"></i>&nbsp;&nbsp;@i.description
            </a>
        </div>
        <div class=""><a href="/@ViewBag.meta/@i.meta/@i.id" class="fh5co_good_font"> @i.name </a></div>
    </div>
}


